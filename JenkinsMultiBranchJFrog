def semanticVersion = "${env.BUILD_NUMBER}.0.0"
def packageName1 = "JenkinsDevOpsProject-1.36.0_pkg_${env.BUILD_NUMBER}"
def packageName2 = "JFrogDefaultDocker_pkg_${env.BUILD_NUMBER}"
def version = "${env.BUILD_NUMBER}.0.0"

pipeline {
    agent any
    tools { 
        maven 'Maven' 
    }
    stages {
        
        stage('Upload-JFrog-Prod') {
            steps {
                echo 'uploading jfrog artifact......'
            }
            
            post {
                always {
                    script {
                             def server = Artifactory.server 'JFROG1'
                             def uploadSpec= """{
                                "files": [{
                                   "pattern": "/tmp/Multiple_repo_Same_Name.jar",
                                   "target":"sunil-multibranch-maven/${env.BUILD_NUMBER}/"
                                }]
                             }"""
            
                          def buildInfo = server.upload(uploadSpec)
                          server.publishBuildInfo buildInfo   
                        }
                    }
               }
            
        }//end of stage
        stage('Download-JFrog-Prod') {
            steps {
                echo 'downloading jfrog artifact from jfrog'
                sleep 10
            }
            
            post {
                  always {       
                   script {
                             def server = Artifactory.server 'JFROG1'
                             def downSpec= """{
                                "files": [{
                                   "pattern": "sunil-multibranch-maven/${env.BUILD_NUMBER}/Multiple_repo_Same_Name.jar",
                                   "target":"/var/jenkins_home/workspace/_JFrog-MultiBranch-Pipeline_prod/JFrog/${env.BUILD_NUMBER}/"
                                }]
                             }"""
            
                          def buildInfo = server.download(downSpec)
                          server.publishBuildInfo buildInfo              
                    } // end of script
                    
                   } //end of always
            }//end of post
       } //end of stage
    }
 }
