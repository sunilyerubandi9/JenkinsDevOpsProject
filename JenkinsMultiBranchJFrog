def semanticVersion = "${env.BUILD_NUMBER}.0.0"
def packageName1 = "JenkinsDevOpsProject-1.36.0_pkg_${env.BUILD_NUMBER}"
def packageName2 = "JFrogDefaultDocker_pkg_${env.BUILD_NUMBER}"
def version = "${env.BUILD_NUMBER}.0.0"

pipeline {
    agent any
    tools { 
        maven 'Maven' 
    }
    stages {
        
        stage('Download-JFrog-Master') {
            steps {
                echo 'downloading jfrog artifact from jfrog'
            }
            
            post {
                  always {       
                   script {
                             def server = Artifactory.server 'devopsbalujfrog'
                             def downSpec= """{
                                "files": [{
                                   "pattern": "sunil-multibranch-maven/5/JenkinsDevOpsProject-1.36.0-SNAPSHOT.jar",
                                   "target":"/var/jenkins_home/workspace/Frog-MultiBranch-Pipeline_master/Jfrog/${env.BUILD_NUMBER}/"
                                }]
                             }"""
            
                          def buildInfo = server.download(downSpec)
                          server.publishBuildInfo buildInfo              
                    } // end of script
                    
                   } //end of always
            }//end of post
       } //end of stage
    }
 }
